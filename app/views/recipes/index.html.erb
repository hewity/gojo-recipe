<h1>All Recipes</h1>
<%= link_to 'My Recipe', controller: 'recipes' %>
<%= link_to 'New recipe', new_recipe_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>

  <% @recipes.each do |recipe| %>
  <tr>
    <td><%= recipe.title %></td>
    <td><%= recipe.text %></td>
  </tr>
  <% end %
</table>
